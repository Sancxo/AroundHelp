    "lng"
  ],
  "properties": {
    "lat": {
      "type": "number",
      "default": 0,
      "description": "Address latitude"
    },
    "lng": {
      "type": "number",
      "default": 0,
      "description": "Address longitude"
    }
  },
  "additionalProperties": false
}